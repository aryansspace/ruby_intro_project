<h1>Countries#index</h1>
<p>Find me in app/views/countries/index.html.erb</p>
<div>
    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th>Country Name</th>
            <th>Country Code</th>
            <th></th>
        </tr>
        </thead>
        <% @countries.each do |country| %>
        <tr>
            <td><%=country.name%></td>
            <td><%=country.code%></td>
            <td><%=link_to "Link for more info", countries_show_path(country)%></td>
        </tr>
        <%end%>
      </table>
 </div>